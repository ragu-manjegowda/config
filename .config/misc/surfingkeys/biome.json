{
    "$schema": "https://biomejs.dev/schemas/1.9.1/schema.json",
    "organizeImports": {
        "enabled": true
    },
    "linter": {
        "enabled": true,
        "ignore": []
    },
    "formatter": {
        "enabled": true,
        "indentStyle": "space",
        "indentWidth": 2,
        "lineWidth": 90,
        "lineEnding": "lf",
        "ignore": []
    },
    "javascript": {
        "formatter": {
            "trailingCommas": "es5",
            "jsxQuoteStyle": "double",
            "semicolons": "asNeeded",
            "quoteStyle": "single"
        }
    }
}
