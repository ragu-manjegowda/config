            "~/.local/share/nvim/lazy/telescope-fzf-native.nvim",
            "~/.local/share/nvim/lazy/telescope-hop.nvim",
            "~/.local/share/nvim/lazy/telescope-live-grep-args.nvim",
            "~/.local/share/nvim/lazy/telescope-ui-select.nvim",
            "~/.local/share/nvim/lazy/trld.nvim",
            "~/.local/share/nvim/lazy/windline.nvim",
            "${3rd}/busted/library",
