{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "completion": {
        "callSnippet": "Both"
    },
    "diagnostics": {
        "enable": true,
        "globals": [
            "vim",
            "capabilities",
            "pcall",
            "require"
        ],
        "neededFileStatus": {
            "codestyle-check": "Any"
        }
    },
    "workspace": {
        "checkThirdParty": false,
        "library": [
            "/usr/share/nvim/runtime",
            "~/.local/share/nvim/lazy/neodev.nvim/types/stable",
            "~/.local/share/nvim/lazy/specs.nvim",
            "~/.local/share/nvim/lazy/telescope.nvim",
            "~/.local/share/nvim/lazy/which-key.nvim",
            "~/.local/share/nvim/lazy/FTerm.nvim",
            "~/.local/share/nvim/lazy/LuaSnip",
            "~/.local/share/nvim/lazy/clangd_extensions.nvim",
            "~/.local/share/nvim/lazy/cmp-buffer",
            "~/.local/share/nvim/lazy/cmp-cmdline",
            "~/.local/share/nvim/lazy/cmp-nvim-lsp",
            "~/.local/share/nvim/lazy/cmp-nvim-lsp-document-symbol",
            "~/.local/share/nvim/lazy/cmp-nvim-lua",
            "~/.local/share/nvim/lazy/cmp-path",
            "~/.local/share/nvim/lazy/firenvim",
            "~/.local/share/nvim/lazy/gitsigns.nvim",
            "~/.local/share/nvim/lazy/lightspeed.nvim",
            "~/.local/share/nvim/lazy/lsp-colors.nvim",
            "~/.local/share/nvim/lazy/lspkind-nvim",
            "~/.local/share/nvim/lazy/lspsaga.nvim",
            "~/.local/share/nvim/lazy/mason-tool-installer.nvim",
            "~/.local/share/nvim/lazy/mason.nvim",
            "~/.local/share/nvim/lazy/neodev.nvim",
            "~/.local/share/nvim/lazy/nvim-cmp",
            "~/.local/share/nvim/lazy/nvim-dap",
            "~/.local/share/nvim/lazy/nvim-dap-ui",
            "~/.local/share/nvim/lazy/nvim-dap-virtual-text",
            "~/.local/share/nvim/lazy/nvim-lspconfig",
            "~/.local/share/nvim/lazy/nvim-tree.lua",
            "~/.local/share/nvim/lazy/nvim-treesitter",
