{
    "Lua.diagnostics.globals": [
        "awesome",
        "button",
        "client",
        "clientbuttons",
        "clientkeys",
        "dbus",
        "message",
        "mouse",
        "root",
        "screen",
        "startup"
    ],
    "diagnostics": {
        "enable": true,
        "globals": [
            "awesome",
            "button",
            "client",
            "clientbuttons",
            "clientkeys",
            "dbus",
            "message",
            "mouse",
            "root",
            "screen",
            "startup"
        ],
        "neededFileStatus": {
            "codestyle-check": "Any"
        }
    },
    "runtime": {
        "path": [
            "/usr/share/awesome/lib/?.lua"
        ],
        "pathStrict": false,
        "version": "LuaJIT"
    },
    "workspace": {
        "checkThirdParty": false,
        "library": [
            "runtime/lua",
            "/usr/share/awesome/lib"
        ],
        "maxPreload": 2000,
        "preloadFileSize": 1000
    }
}
